{
  "version": 2,
  "buildCommand": "pnpm run build",
  "outputDirectory": "public",
  "rewrites": [
    { "source": "/api/oauth/:path*", "destination": "/api/index.js" },
    { "source": "/api/trpc/:path*", "destination": "/api/index.js" },
    { "source": "/:path((?!api/).*)", "destination": "/index.html" }
  ]
}
